{
    "name": "jrobeson/sylius-settings-bundle",
    "type": "symfony-bundle",
    "description": "Settings system for Symfony2 applications, editable via web user interface.",
    "keywords": ["config", "configuration", "settings"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name": "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name": "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^7.1",

        "sylius/registry": "^1.0",
        "sylius/resource-bundle": "^1.0",
        "symfony/framework-bundle": "^3.3"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.1",
        "symfony/form": "^3.3",
        "symfony/validator": "^3.3"
    },
     "autoload": {
        "psr-4": { "Sylius\\Bundle\\SettingsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Sylius\\Bundle\\SettingsBundle\\spec\\": "spec/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
